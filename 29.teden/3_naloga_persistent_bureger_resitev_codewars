import java.util.*;

class Persist {
	public static int persistence(long n) {
		
		int result = 0;
		while (n > 9) {
			long multi = 1;
			for (char t : String.valueOf(n).toCharArray()) {
				multi *= Long.parseLong(String.valueOf(t));
			}
			n = multi;
			result++;
		}
		return result;
    
	}
}